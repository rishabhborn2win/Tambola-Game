<img src={tambolaWrite} className=\"Logo circle-img\" alt=\"logo\" />\n      <CSSTransition\n        in={!isSmallScreen || isNavVisible}\n        timeout={350}\n        classNames=\"NavAnimation\"\n        unmountOnExit\n      >\n        <nav className=\"Nav\">\n          <a href=\"/\" onClick={(e) => onOpenModal(e)}>Rules</a>\n          <a href=\"/\" onClick={(e) =>  e.preventDefault()}>About Us</a>\n          <a href=\"/\"onClick={(e) =>  e.preventDefault()}>Help</a>\n          <button>\n            <a href=\"#top\" onClick={(e) => resetGame(e)}>\n              Click here to Reset the app!\n            </a>\n          </button>\n        </nav>\n      </CSSTransition>\n      <button onClick={toggleNav} className=\"Burger\">\n        &#9776;\n      </button>\n    </header>\n  );\n}\n","/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Heading.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Rules.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/footer/Footer.js",["98"],{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","severity":1,"message":"104","line":100,"column":22,"nodeType":"105","messageId":"106","endLine":100,"endColumn":24},{"ruleId":"107","severity":1,"message":"108","line":683,"column":15,"nodeType":"109","endLine":683,"endColumn":30},{"ruleId":"110","severity":1,"message":"111","line":6,"column":8,"nodeType":"112","messageId":"113","endLine":6,"endColumn":22},{"ruleId":"110","severity":1,"message":"114","line":10,"column":8,"nodeType":"112","messageId":"113","endLine":10,"endColumn":9},{"ruleId":"107","severity":1,"message":"108","line":14,"column":106,"nodeType":"109","endLine":14,"endColumn":121},"no-native-reassign",["115"],"no-negated-in-lhs",["116"],"array-callback-return","Array.prototype.map() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","react/jsx-no-target-blank","U