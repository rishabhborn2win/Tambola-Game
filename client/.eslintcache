ay\" component={Board} />\n        <Footer></Footer>\n      </Router>\n    </Provider>\n  );\n}\n\nexport default App;\n","/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/store.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Home.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/JoinGameForm.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/HostForm.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Board.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/routing/PrivateRoute.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/layout/Alert.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/actions/game.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Player.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/layout/Spinner.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/actions/types.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/actions/alert.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/reducers/index.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/reducers/game.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/reducers/alert.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Host.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/header/Header.js",["117"],"/* eslint-disable jsx-a11y/accessible-emoji */\nimport React, { useState, useEffect } from \"react\";\nimport \"./Header.css\";\nimport { CSSTransition } from \"react-transition-group\";\nimport tambolaWrite from \"./tambola-write.png\";\nimport { Modal } from \"react-responsive-modal\";\nimport \"react-responsive-modal/styles.css\";\nimport Rules from \"../Rules\";\nimport { Link } from \"react-router-dom\";\n\nexport default function Header() {\n  const [isNavVisible, setNavVisibility] = useState(false);\n  const [isSmallScreen, setIsSmallScreen] = useState(false);\n\n  useEffect(() => {\n    const mediaQuery = window.matchMedia(\"(max-width: 700px)\");\n    mediaQuery.addListener(handleMediaQueryChange);\n    handleMediaQueryChange(mediaQuery);\n\n    return () => {\n      mediaQuery.removeListener(handleMediaQueryChange);\n    };\n  }, []);\n\n  const handleMediaQueryChange = (mediaQuery) => {\n    if (mediaQuery.matches) {\n      setIsSmallScreen(true);\n    } else {\n      setIsSmallScreen(false);\n    }\n  };\n\n  const toggleNav = () => {\n    setNavVisibility(!isNavVisible);\n  };\n\n  //Reset the app on local system\n  const resetGame = (e) => {\n    e.preventDefault();\n    prompt(\"Are You sure?\", \"\");\n    localStorage.removeItem(\"playerid\");\n    localStorage.removeItem(\"gameid\");\n    localStorage.removeItem(\"username\");\n  };\n\n  const [open, setOpen] = React.useState(true);\n  const onCloseModal = () => {\n    setOpen(false);\n  };\n  const onOpenModal = (e) => {\n    e.preventDefault();\n    setOpen(true);\n  };\n\n  return (\n    <header className=\"Header\">\n      <Modal open={open} onClose={onCloseModal} center>\n        <Rules function={onCloseModal} />\n      </Modal>\n      <Link to=\"/\">\n        <img src={tambolaWrite} className=\"Logo circle-img\" alt=\"logo\" />\n      </Link>\n      <CSSTransition\n        in={!isSmallScreen || isNavVisible}\n        timeout={350}\n        classNames=\"NavAnimation\"\n        unmountOnExit\n      >\n        <nav className=\"Nav\">\n          <a href=\"/\" onClick={(e) => onOpenModal(e)}>\n            Rules\n          </a>\n          {/* <a href=\"/aboutus\" onClick={(e) => e.preventDefault()}> */}\n          <Link to=\"/aboutus\">About Us</Link>\n          <Link to=\"/generate/ticket\">Generate!</Link>\n\n          {/* </a> */}\n          {/* <a href=\"/aboutus\" onClick={(e) => e.preventDefault()}> */}\n          <Link to=\"/help\">Help</Link>\n          {/* </a> */}\n          {/* <button>\n            <a href=\"#top\" onClick={(e) => resetGame(e)}>\n              Click here to Reset the app!\n            </a>\n          </button> */}\n        </nav>\n      </CSSTransition>\n      <button onClick={toggleNav} className=\"Burger\">\n        &#9776;\n      </button>\n    </header>\n  );\n}\n","/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Heading.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Help.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/AboutUs.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/footer/Footer.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Rules.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/GenerateTicketForm.js",["118"],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Ticket.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/TicketList.js",["119"],"import React, { useEffect } from \"react\";\nimport Ticket from \"./Ticket\";\nimport { connect } from \"react-redux\";\nimport PropTypes from \"prop-types\";\nimport { loadTicket } from \"../actions/game\";\nimport Heading from \"./Heading\";\n\nconst TicketList = ({ tickets, loadTicket }) => {\n  useEffect(() => {\n    loadTicket();\n  }, []);\n\n  return (\n    <div>\n      <Heading text=\"Ticket List!\" />\n      <div>\n        {tickets\n          ? tickets.tickets !== undefined\n            ? tickets.tickets.map((serial, index) => {\n                return (\n                  <div>\n                    <p>Name: {tickets.name}</p>\n                    <p>Ticket ID: {`${tickets.ticketId}-${index + 1}`}</p>\n                    <Ticket ticket={serial} />\n                  </div>\n                );\n     