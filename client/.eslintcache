 ...formData,\n      playername: e.target.value,\n      playerid: e.target.options[e.target.selectedIndex].title,\n    };\n    console.log(data);\n    setFormData(data);\n  };\n  const decrement = () => {\n    if(noOfTickets > 0)\n    setFormData({...formData, noOfTickets: noOfTickets-1});\n  }\n\n  const increment = () => {\n    if(noOfTickets < 6)\n    setFormData({...formData, noOfTickets: noOfTickets+1});\n  }\n  return (\n    <div className=\"content-post\">\n      <Fragment>\n        <Heading text=\"Generate Ticket\" />\n        {/* <span class=\"text-span\">Join Game Form:-</span> */}\n        <div className=\"container\">\n          <form onSubmit={(e) => onSubmit(e)}>\n            <div class=\"select-list\">\n              <select\n                value={playername}\n                className=\"custom-select\"\n                id={playerid}\n                onChange={(e) => handleChange(e)}\n              >\n                <option value={\"not allowed\"} title={\"\"}>\n                  *Select Player!\n                </option>\n                {players.map((player) => {\n                  if (!player.tickets) {\n                    return (\n                      <option value={player.name} title={player._id}>\n                        {player.name}\n                      </option>\n                    );\n                  } else return 0;\n                })}\n              </select>\n            </div>\n            <div class=\"number-selector\">\n              <span onClick={decrement} >-</span>\n              <span>{noOfTickets}</span>\n              <span onClick={increment}>+</span>\n              {/* onClick={setFormData({...formData, noOfTickets: noOfTickets+1})} */}\n              {/* <label class=\"omrs-input-underlined\">\n                <input\n                  className=\"input\"\n                  placeholder=\"No. Of Tickets\"\n                  type=\"text\"\n                  id=\"noOfTickets\"\n                  name=\"noOfTickets\"\n                  onChange={(e) => onChange(e)}\n                ></input>\n              </label> */}\n            </div>\n            <input\n              type=\"submit\"\n              value=\"Generate\"\n              className=\"btn btn-primary btn-m-20\"\n            ></input>\n          </form>\n        </div>\n      </Fragment>\n    </div>\n  );\n};\nGenerateTicketForm.prototype = {\n  notifyFill: PropTypes.func.isRequired,\n  generateTicket: PropTypes.func.isRequired,\n};\nconst mapStateToProps = (state) => ({\n  tickets: state.game.tickets,\n});\n\nexport default connect(mapStateToProps, { notifyFill, generateTicket })(\n  withRouter(GenerateTicketForm)\n);\n","/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/NumberHistory.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/transformFunction.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/layout/Error.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/TicketList.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Ticket.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/DividendsVerify.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Leadarboard.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/GameOver.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/scanQR/Ticket.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/speechRecognition/SpeechRecognition.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/HowToPlay.js",[],{"ruleId":"154","replacedBy":"155"},{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","severity":1,"message":"159","line":121,"column":10,"nodeType":"160","messageId":"161","endLine":121,"endColumn":23},{"ruleId":"158","severity":1,"message":"162","line":4,"column":1