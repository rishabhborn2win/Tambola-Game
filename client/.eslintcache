gment>;\n    // else {\n    return (\n      <Fragment>\n        <Heading text={`Game Dashboard (${typeOfPlayer})`} />\n\n        {game.loading ? (\n          <Spinner></Spinner>\n        ) : (\n          <div className=\"container\">\n            <Link to=\"/play\">\n              <button className=\"btn btn-lg\">Resume Game</button>\n              <br></br>\n              <br></br>\n            </Link>\n            <div className=\"host-player\">\n              <Host game={game.game} total={numCalled.length}></Host>\n              {game.game ? (\n                localStorage.gameid === game.game._id ? (\n                  <Player game={game.game ? game.game : {}} />\n                ) : (\n                  \"\"\n                )\n              ) : (\n                \"\"\n              )}\n            </div>\n          </div>\n        )}\n      </Fragment>\n    );\n    // }\n  }\n\n  return (\n    <Fragment>\n      <Heading text=\"Welcome to Tambola,\" />\n      <span class=\"text-span\">Select Option:-</span>\n      <div class=\"container\">\n        <Link to=\"/join\">\n          <Button variant=\"success\" className=\"button-lg\">\n            Join Game\n          </Button>\n        </Link>{\" \"}\n        <Link to=\"/create\">\n          <Button variant=\"success\" className=\"button-lg\">\n            Create A Game\n          </Button>\n        </Link>\n      </div>\n    </Fragment>\n  );\n}\nHome.propTypes = {\n  game: PropTypes.object.isRequired,\n};\n\nconst mapStateToProps = (state) => ({\n  game: state.game,\n});\n\nexport default connect(mapStateToProps)(Home);\n","/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/AboutUs.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/JoinGameForm.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/HostForm.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Help.js",[],"/Users/rishabhmishra/Documents/GitH