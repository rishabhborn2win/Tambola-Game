  };\n\n  const toggleNav = () => {\n    setNavVisibility(!isNavVisible);\n  };\n\n  //Reset the app on local system\n  const resetGame = (e) => {\n    e.preventDefault();\n    prompt(\"Are You sure?\", \"\");\n    localStorage.removeItem(\"playerid\");\n    localStorage.removeItem(\"gameid\");\n    localStorage.removeItem(\"username\");\n  };\n\n  const [open, setOpen] = React.useState(true);\n  const onCloseModal = () => {\n    setOpen(false);\n  };\n  const onOpenModal = (e) => {\n    e.preventDefault();\n    setOpen(true);\n  };\n\n  return (\n    <header className=\"Header\">\n      <Modal open={open} onClose={onCloseModal} center>\n        <Rules function={onCloseModal} />\n      </Modal>\n      <Link to=\"/\">\n        <img src={tambolaWrite} className=\"Logo circle-img\" alt=\"logo\" />\n      </Link>\n      <CSSTransition\n        in={!isSmallScreen || isNavVisible}\n        timeout={350}\n        classNames=\"NavAnimation\"\n        unmountOnExit\n      >\n        <nav className=\"Nav\">\n          <a href=\"/\" onClick={(e) => onOpenModal(e)}>\n            Rules\n          </a>\n          {/* <a href=\"/aboutus\" onClick={(e) => e.preventDefault()}> */}\n          <Link to=\"/aboutus\">About Us</Link>\n          {/* </a> */}\n          {/* <a href=\"/aboutus\" onClick={(e) => e.preventDefault()}> */}\n          <Link to=\"/help\">Help</Link>\n          {/* </a> */}\n          <button>\n            <a href=\"#top\" onClick={(e) => resetGame(e)}>\n              Click here to Reset the app!\n            </a>\n          </button>\n        </nav>\n      </CSSTransition>\n      <button onClick={toggleNav} className=\"Burger\">\n        &#9776;\n      </button>\n    </header>\n  );\n}\n","/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Heading.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/Help.js",[],"/Users/rishabhmishra/Documents/GitHub/Tambola-Game/client/src/components/AboutUs.js",["108"],"import Re